<%= render "layouts/errors", :object => @user %>

<%= form_for @user do |c| %>
    <h4><%= c.label :name, class: "label label-default" %> </h4>
    <%= c.text_field :name, class: "contacttextfield" %>
    <h4><%= c.label :email, class: "label label-default" %> </h4>
    <%= c.text_field :email, class: "contacttextfield" %>
    <h4><%= c.label :password, class: "label label-default" %> </h4>
    <%= c.password_field :password, class: "contacttextfield" %> <br \>
    <%= c.submit  "Create", class: "btn btn-primary" %>
<%end%>